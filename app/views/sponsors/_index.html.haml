          = image_tag('/assets/sponsors/logo/mysql.png')
      .span2#microsoft
        = link_to 'http://www.microsoft.com/en-us/openness/default.aspx#home', target: :blank do
          = image_tag('/assets/sponsors/logo/microsoft.png')
    .span12.divider-space
    #bronze.text-center
      .span3
        = image_tag('/assets/sponsors/rockhopper.png', width: 300)
      .span12.divider-space
